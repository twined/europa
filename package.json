{
  "name": "@brandocms/europacss",
  "version": "0.13.1",
  "description": "Europa CSS system",
  "main": "lib/index.js",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "helpers"
  ],
  "scripts": {
    "dev": "babel src --out-dir lib --copy-files && yalc push",
    "test": "jest __tests__",
    "coverage": "jest __tests__ --coverage",
    "prebuild": "rimraf lib",
    "build": "babel src --out-dir lib --copy-files"
  },
  "author": "Univers <mail@univers.agency>",
  "license": "MIT",
  "repository": "https://github.com/brandocms/europacss.git",
  "bugs": {
    "url": "https://github.com/brandocms/europacss/issues"
  },
  "homepage": "https://github.com/brandocms/europacss",
  "files": [
    "lib",
    "stubs"
  ],
  "dependencies": {
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.31",
    "postcss-extend-rule": "^3.0.0",
    "postcss-functions": "twined/postcss-functions",
    "postcss-nested": "^4.2.1",
    "postcss-reporter": "^6.0.1",
    "reduce-css-calc": "^2.1.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "babel-jest": "^26.6.3",
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-es5": "^1.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/jest/customMatchers.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/fixtures/"
    ]
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "browserslist": [
    "> 1%"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "8.9.0"
          }
        }
      ]
    ]
  }
}
